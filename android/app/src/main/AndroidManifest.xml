<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.phoenix.lotterys"
    xmlns:tools="http://schemas.android.com/tools">


    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />

    <!--9.0IntentService -->
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <!--8.0安装需要的权限-->
    <uses-permission android:name="android.permission.REQUEST_INSTALL_PACKAGES" />
    <!--    <uses-permission android:name="android.permission.PACKAGE_USAGE_STATS"-->
    <!--    tools:ignore="ProtectedPermissions" />-->
    <uses-permission android:name="android.permission.READ_SETTINGS" />
    <uses-permission
        android:name="android.permission.WRITE_SETTINGS"
        tools:ignore="ProtectedPermissions" />
    <uses-permission
        android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"
        tools:ignore="ProtectedPermissions" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>



    <application
      android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="${app_icon1}"
        android:label="${app_name1}"
        android:networkSecurityConfig="@xml/network_security_config"
        android:supportsRtl="true"
        android:resizeableActivity="false"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        tools:replace="android:appComponentFactory,android:icon, android:theme , android:label"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning,">

      <activity
        android:name=".MainActivity"
        android:label="@string/app_name"
        android:configChanges="keyboard|keyboardHidden|orientation|screenSize"
        android:windowSoftInputMode="adjustResize">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
      </activity>
      <activity android:name="com.facebook.react.devsupport.DevSettingsActivity" />


        <activity
            android:name="com.phoenix.lotterys.main.LaunchActivity"
            android:configChanges="keyboardHidden"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme">
        </activity>

        <activity
            android:name="com.phoenix.lotterys.main.MainActivity"
            android:configChanges="orientation|screenSize"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize"
            >
        </activity>
        <activity android:name="com.phoenix.lotterys.buyhall.activity.TicketDetailsActivity"
            android:configChanges="orientation|screenSize"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize">
        </activity>
        <activity
            android:name="com.phoenix.lotterys.my.activity.LoginActivity"
            android:configChanges="keyboardHidden"
            android:windowSoftInputMode="adjustResize"
            android:screenOrientation="portrait">
            <!--android:windowSoftInputMode="adjustResize"-->
        </activity>
        <activity
            android:name="com.phoenix.lotterys.my.activity.RegeditActivity"
            android:configChanges="keyboardHidden"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name="com.phoenix.lotterys.main.webview.GoWebActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait">

        </activity>

        <!--        优惠券详情-->
        <activity
            android:name="com.phoenix.lotterys.main.webview.CouponWebActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name="com.phoenix.lotterys.main.webview.GoWebActivity1"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name="com.phoenix.lotterys.home.activity.ElectronicActivity"
            android:configChanges="keyboardHidden"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name="com.phoenix.lotterys.buyhall.activity.SelectLotteryTicketActivity"
            android:theme="@style/TransparentTheme" />
        <activity
            android:name="com.phoenix.lotterys.coupons.activity.WebActivity"
            android:theme="@style/TransparentTheme" />
        <activity
            android:name="com.phoenix.lotterys.coupons.activity.RichTextActivity"
            android:configChanges="keyboardHidden"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.phoenix.lotterys.my.activity.DepositActivity"
            android:configChanges="keyboardHidden"
            android:screenOrientation="portrait"/>
        <!--//额度转换-->

        <service android:name="com.phoenix.lotterys.view.MyIntentService"/>

        <!--公用act-->
        <activity
            android:name="com.phoenix.lotterys.main.FragmentUtilAct"
            android:configChanges="keyboardHidden"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustResize" />

        <activity
            android:name=".chat.ChatActivity"
            android:configChanges="keyboardHidden"
            android:screenOrientation="portrait"/>

        <!--利息宝-->
        <activity
            android:name=".my.activity.InterestDoteyActivity"
            android:configChanges="keyboardHidden"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".main.webview.AgentWebActivity"
            android:hardwareAccelerated="true"
            android:label="Gif"
            android:screenOrientation="sensor"
            android:configChanges="orientation|screenSize|keyboardHidden">
        </activity>
        <!--受益报表-->
        <activity
            android:name=".my.activity.ReportRecordActivity"
            android:configChanges="keyboardHidden"
            android:screenOrientation="portrait"/>
        <!--转入转出记录-->
        <activity
            android:name=".my.activity.ReportingRecordActivity"
            android:configChanges="keyboardHidden"
            android:screenOrientation="portrait"/>
        <!--转入利息宝-->
        <activity
            android:name=".my.activity.RollInOutActivity"
            android:configChanges="keyboardHidden"
            android:screenOrientation="portrait"/>
        <!--额度转换-->
        <activity
            android:name=".my.activity.LimitTransformActivity"
            android:configChanges="keyboardHidden"
            android:screenOrientation="portrait"/>
        <!--秒秒彩-->
        <activity
            android:name="com.phoenix.lotterys.buyhall.activity.SecsecActivity"
            android:configChanges="keyboardHidden"
            android:theme="@style/TransparentTheme"
            android:screenOrientation="portrait"/>

        <!--支付类型 线上-->
        <activity
            android:name=".my.activity.PayOnLineActivity"
            android:configChanges="keyboardHidden"
            android:screenOrientation="portrait">
        </activity>
        <!--支付类型 线下-->
        <activity
            android:name=".my.activity.PayOffLineActivity"
            android:configChanges="keyboardHidden"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name=".home.activity.InformationActivity"
            android:configChanges="keyboardHidden"
            android:screenOrientation="portrait">
        </activity>
        <!--彩票游戏大厅-->
        <activity
            android:name=".buyhall.activity.LotteryHallTypeActivity"
            android:configChanges="keyboardHidden"
            android:screenOrientation="portrait">
        </activity>
        <!--电子捕鱼棋牌体育真人游戏列表-->
        <activity
            android:name=".buyhall.activity.GameGallTypeActivity"
            android:configChanges="keyboardHidden"
            android:screenOrientation="portrait">
        </activity>
        <!--游戏大厅-->
        <activity
            android:name=".buyhall.activity.BuyHallTypeActivity"
            android:configChanges="keyboardHidden"
            android:screenOrientation="portrait">
        </activity>
        <!--黑色模板首页-->
        <activity
            android:name="com.phoenix.lotterys.main.BlackMainActivity"
            android:configChanges="orientation|screenSize"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize"
            >
        </activity>
        <!--黑色模板登录-->
        <activity
            android:name="com.phoenix.lotterys.my.activity.BlackLoginActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize">
        </activity>

        <activity
            android:name=".home.activity.PostMessageActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"
            >

        </activity>
        <activity
            android:name=".home.activity.ReviewActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity
            android:name="com.phoenix.lotterys.my.fragment.MessagPopFrag"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"
            android:theme="@style/TranslucentTheme"
            >
        </activity>
    </application>

</manifest>
