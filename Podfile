platform :ios, '9.0'
target "ug" do
    use_frameworks!
    #消除第三方库警告
inhibit_all_warnings!

 # 布局自动适应库
pod 'Masonry'
 # 类似Android提示库
pod 'Toast', '~> 4.0.0'
 # 键盘库
pod 'IQKeyboardManager'
 # alert提示框
pod 'LEEAlert'

  # 防奔溃
    pod 'AvoidCrash'

   # 线下开发、测试工具组
    pod 'DoraemonKit/Core', '~> 1.1.4', :configurations => ['Debug']
    pod 'DoraemonKit/WithLogger', '~> 1.1.4', :configurations => ['Debug']


end
