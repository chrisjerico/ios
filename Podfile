platform :ios, '9.0'
target "ug" do
    use_frameworks!
    #消除第三方库警告
inhibit_all_warnings!

 # 布局自动适应库
pod 'Masonry'
 # 类似Android提示库
pod 'Toast', '~> 4.0.0'
 # 键盘库
pod 'IQKeyboardManager'
 # alert提示框
pod 'LEEAlert'
 # 防奔溃
pod 'AvoidCrash'
 #web浏览器
pod 'BAWKWebView' 


# fish新增
pod 'JRSwizzle'         # 方法交换库
pod 'Aspects'           # 监听函数回调到Block
pod 'RegExCategories'   # 正则表达式库
pod 'YYCategories'      # Category 类型工具库（YYKit 组件之一）
pod 'DateTools'         # 日期工具
pod 'MJExtension'       # 字典转数据模型库
pod 'FSCalendar'        # 日历控件
 #路由器
pod 'FFRouter'

end
